{
  "name": "nodejs-api",
  "version": "1.0.0",
  "description": "REST API made with NODE | Express | MongoDB",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "client-install": "npm install --prefix client",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Shreyas Balachandran",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.0",
    "nodemon": "^1.19.1"
  }
}
